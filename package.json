{
  "name": "vlibras-translator-api",
  "description": "VLibras Translation Service API",
  "version": "2.4.0",
  "private": true,
  "type": "module",
  "contributors": [
    "Wesnydy Ribeiro <wesnydy@lavid.ufpb.br>",
    "Jonathan Brilhante <jonathan.brilhante@lavid.ufpb.br>"
  ],
  "scripts": {
    "dev": "DEBUG=vlibras-translator-*:* NODE_ENV=dev nodemon --watch src src/index.js ",
    "start": "DEBUG=vlibras-translator-api:info NODE_ENV=production node src/index.js",
    "migration": "DEBUG=vlibras-migration:info NODE_ENV=dev node src/app/db/postgresMigration/migration.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --coverage",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=v18.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "dotenv": "^16.4.5",
    "express": "~4.18.3",
    "express-validator": "^7.0.0",
    "helmet": "^7.1.0",
    "http-errors": "~2.0.0",
    "ioredis": "^5.3.2",
    "mongoose": "^5.9.16",
    "morgan": "~1.10.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.4",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@jest/globals": "^29.7.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
